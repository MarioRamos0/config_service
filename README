# Comandos a ejcutar 

export POSTGRES_USER=postgres
export POSTGRES_DB=db_name
export PORT=8085

echo -n "jwtsecreta" > jwt_secret
echo -n "1234" > db_user_passwd
echo -n "postgresql+psycopg2://${POSTGRES_USER}:$(cat POSTGRES_PASSWORD)@postgres-db:5432/${POSTGRES_DB}" > database_url


# Configuraciones ha proxy